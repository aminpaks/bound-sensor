{
    "compilerOptions": {
        "sourceMap": true,
        "inlineSources": true,
        "declaration": true,
        "noImplicitAny": false,
        "skipLibCheck": true,
        "stripInternal": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "suppressImplicitAnyIndexErrors": true,
        "module": "es2015",
        "moduleResolution": "node",
        "lib": ["es2015", "dom"],
        "target": "es5",
        "outDir": "../lib-es5/",
        "rootDir": "./",
        "baseUrl": "./",
        "types": []
    },
    "files": [
        "./index.ts"
    ],
    "angularCompilerOptions": {
        "strictMetadataEmit": true,
        "skipTemplateCodegen": true,
        "annotateForClosureCompiler": true,
        "flatModuleOutFile": "bound-sensor.es5.js",
        "flatModuleId": "bound-sensor",
        "genDir": "../"
    }
}