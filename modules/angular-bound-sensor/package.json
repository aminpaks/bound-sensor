{
  "name": "angular-bound-sensor",
  "version": "1.0.1",
  "description": "Find out once boundary size of element changes in Angular",
  "author": "aminpaks <amin.pakseresht@hotmail.com>",
  "license": "MIT",
  "main": "dist/bundles/angular-bound-sensor.umd.min.js",
  "module": "dist/angular-bound-sensor.es5.js",
  "es2015": "dist/angular-bound-sensor.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "npm-run-all clean:*",
    "clean:dist": "rimraf ./dist",
    "clean:tsc": "rimraf ./tsc-out",
    "inline:src": "ng2-inline -b src -r -o tsc-out/lib 'src/**/*.ts'",
    "rollup:builds": "ts-node ./builder/rollup-build.ts",
    "bundle": "npm-run-all clean inline:* rollup:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aminpaks/angular-bound-sensor.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "container element resize",
    "resize event for angular2",
    "resize sensor for angular2",
    "element resize event"
  ],
  "dependencies": {
    "bound-sensor": "^1.0.0",
    "@angular/core": ">= 4.0.0 < 5",
    "rxjs": "^5.0.0",
    "zone.js": "^0.8.0"
  },
  "peerDependencies": {
    "@angular/core": "^4.0.0",
    "rxjs": "^5.0.0",
    "zone.js": "^0.8.0"
  },
  "devDependencies": {
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/lodash-es": "^4.14.5",
    "@types/node": "^8.0.8",
    "@types/rollup": "^0.41.1",
    "angular2-inline-template-style": "^1.1.0",
    "camelcase": "^4.1.0",
    "chokidar": "^1.7.0",
    "lodash-es": ">=4.0.0 <5.0.0",
    "magic-string": "^0.22.2",
    "npm-run-all": "^4.0.2",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "ts-node": "^3.2.0",
    "tslint": "^5.4.2",
    "typescript": "~2.3.4"
  }
}
