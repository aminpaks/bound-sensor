{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "stripInternal": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": true,
    "moduleResolution": "node",
    "lib": [
      "es2015",
      "dom"
    ],
    "types": [],
    "baseUrl": ".",
    "rootDir": "./src",
    "module": "es2015",
    "target": "es2015",
    "outDir": "../lib-es2015"
  },
  "files": [
    "./src/index.ts"
  ],
  "angularCompilerOptions": {
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "annotateForClosureCompiler": false,
    "flatModuleOutFile": "angular-bound-sensor.js",
    "flatModuleId": "angular-bound-sensor"
  }
}
